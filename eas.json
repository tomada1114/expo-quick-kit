{
  "cli": {
    "version": ">= 13.0.0",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_REVENUE_CAT_API_KEY_APPLE": "@EXPO_PUBLIC_REVENUE_CAT_API_KEY_APPLE"
      }
    },
    "production": {
      "distribution": "store",
      "env": {
        "EXPO_PUBLIC_REVENUE_CAT_API_KEY_APPLE": "@EXPO_PUBLIC_REVENUE_CAT_API_KEY_APPLE"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "your-apple-id@example.com",
        "ascAppId": "1234567890",
        "appleTeamId": "ABCDE12345"
      }
    }
  }
}
